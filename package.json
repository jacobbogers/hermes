{
  "name": "hermes",
  "version": "1.0.0",
  "description": "express middleware to manage user signup/login and password reset/account activation",
  "main": "makes no sense when we deploy as library ./dist/index.js",
  "scripts": {
    "clear": "rimraf ./dist",
    "prepublish": "echo 'prepublish'",
    "prepare": "echo 'prepare'",
    "prepublishOnly": "echo prepublishOnly",
    "publish": "echo publish",
    "preinstall": "echo preinstall",
    "install": "echo install",
    "postinstall": "npm run fixtypes",
    "preuninstall": "echo preuninstall",
    "uninstall": "echo uninstall",
    "postuninstall": "echo postuninstall",
    "preversion": "echo preversion",
    "version": "echo version",
    "postversion": "echo postversion",
    "pretest": "echo prestest",
    "test": "./node_modules/.bin/mocha ./dist/tests/all-tests.js",
    "posttest": "echo posttest",
    "prestop": "echo prestop",
    "stop": "echo stop",
    "poststop": "echo poststop",
    "prestart": "echo prestart",
    "start": "cross-env DEBUGx=express-session,cookie node dist/server/server.js",
    "start:server": "cross-env DEBUGx=express-session,cookie node dist/server/server.js",
    "poststart": "echo poststart",
    "prerestart": "echo prerestart",
    "restart": "echo restart",
    "postrestart": "echo postrestart",
    "preshrinkwrap": "echo preshrinkwrap",
    "shrinkwrap": "echo shrinkwrap",
    "postshrinkwrap": "echo postshrinkwrap",
    "build:client:dev": "cross-env NODE_ENV=development webpack --config src/config/webpack/client.config",
    "build:client:prod": "cross-env NODE_ENV=production webpack --config src/config/webpack/client.config",
    "build:server:dev": "cross-env NODE_ENV=development webpack --config src/config/webpack/server.config",
    "build:server:prod": "cross-env NODE_ENV=production webpack --config src/config/webpack/server.config",
    "fixtypes": "cpr extra/index.d.ts node_modules/@types/express-session/index.d.ts -o"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobbogers/hermes.git"
  },
  "keywords": [
    "login",
    "signup",
    "sign up",
    "activation",
    "password",
    "authentication",
    "reset",
    "account",
    "access",
    "response email",
    "auth0"
