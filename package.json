{
  "name": "hermes",
  "version": "1.0.0",
  "description": "express middleware to manage user signup/login and password reset/account activation",
  "keywords": [
    "access",
    "account",
    "activation",
    "auth0",
    "authentication",
    "login",
    "password",
    "reset",
    "response email",
    "sign up",
    "signup"
  ],
  "homepage": "https://github.com/jacobbogers/hermes#readme",
  "bugs": {
    "url": "https://github.com/jacobbogers/hermes/issues"
  },
  "license": "MIT",
  "author": "Jacob K.F. Bogers",
  "main": "makes no sense when we deploy as library ./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobbogers/hermes.git"
  },
  "scripts": {
    "build:client:dev": "npm run graphiQLLocal && cross-env NODE_ENV=development webpack --config src/config/webpack/client.config",
    "build:client:prod": "cross-env NODE_ENV=production webpack --config src/config/webpack/client.config",
    "build:server:dev": "cross-env NODE_ENV=development webpack --config src/config/webpack/server.config",
    "build:server:prod": "cross-env NODE_ENV=production webpack --config src/config/webpack/server.config",
    "clear": "rimraf ./dist",
    "fixtypes": "cpr extra/index.d.ts node_modules/@types/express-session/index.d.ts -o",
    "graphiQLLocal": "cpr extra/renderGraphiQL.js node_modules/graphql-server-module-graphiql/dist/renderGraphiQL.js -o",
    "preinstall": "echo preinstall",
    "install": "echo install",
    "lint": "tslint -c tslint.json -p . --type-check",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "echo postinstall && npm run graphiQLLocal",
    "prepare": "echo 'prepare'",
    "prepublish": "echo 'prepublish'",
    "publish": "echo publish",
    "prepublishOnly": "echo prepublishOnly",
    "prerestart": "echo prerestart",
    "restart": "echo restart",
    "postrestart": "echo postrestart",
    "preshrinkwrap": "echo preshrinkwrap",
    "shrinkwrap": "echo shrinkwrap",
    "postshrinkwrap": "echo postshrinkwrap",
    "prestart": "echo prestart",
    "start": "cross-env DEBUGx=express-session,cookie node dist/server/server.js",
    "poststart": "echo poststart",
    "start:server": "cross-env DEBUGx=express-session,cookie node dist/server/server.js",
    "prestop": "echo prestop",
    "stop": "echo stop",
    "poststop": "echo poststop",
    "pre-test": "echo prestest",
    "test": "cross-env TS_NODE_DISABLE_WARNINGS=true mocha",
    "test:w": "npm run test -- -w -R min",
    "post-test": "echo posttest",
    "preuninstall": "echo preuninstall",
    "uninstall": "echo uninstall",
    "postuninstall": "echo postuninstall",
    "preversion": "echo preversion",
    "version": "echo version",
    "postversion": "echo postversion"
  },
  "dependencies": {
    "apollo-client": "^1.9.2",
    "body-parser": "^1.17.2",
    "circular-json": "^0.3.3",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "graphql": "^0.10.5",
    "graphql-server-core": "^1.1.0",
    "graphql-server-express": "^0.8.5",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^1.2.2",
    "install": "^0.9.6",
    "mailgun-js": "^0.10.1",
    "merge2": "^1.1.0",
    "moment": "^2.18.1",
    "npm": "^4.6.1",
    "pg": "^7.3.0",
    "pg-cursor": "^1.3.0",
    "pg-query-stream": "^1.1.1",
    "react": "^15.6.1",
    "react-apollo": "^1.4.15",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "tracer": "^0.8.11",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/chai": "^4.0.4",
    "@types/express": "^4.0.37",
    "@types/express-session": "^1.15.3",
    "@types/graphql": "^0.11.1",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.27",
    "@types/pg": "^7.1.0",
    "@types/pg-pool": "0.0.3",
    "@types/pg-query-stream": "^1.0.1",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-router-dom": "^4.0.5",
    "@types/uid-safe": "^2.1.1",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "cpr": "^2.0.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template": "^6.0.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "lerna": "^2.1.2",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^2.11.0",
    "postcss-loader": "^2.0.5",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^2.3.0",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.5.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-node-externals": "^1.6.0"
  }
}
