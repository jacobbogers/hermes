{
    "name": "hermes",
    "version": "1.0.0",
    "description": "express middleware to manage user signup/login and password reset/account activation",
    "keywords": [
        "access",
        "account",
        "activation",
        "auth0",
        "authentication",
        "login",
        "password",
        "reset",
        "response email",
        "sign up",
        "signup"
    ],
    "homepage": "https://github.com/jacobbogers/hermes#readme",
    "bugs": {
        "url": "https://github.com/jacobbogers/hermes/issues"
    },
    "license": "MIT",
    "author": "Jacob K.F. Bogers",
    "main": "makes no sense when we deploy as library ./dist/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jacobbogers/hermes.git"
    },
    "scripts": {
        "build:client:dev": "npm run graphiQLLocal && cross-env NODE_ENV=development webpack --config src/config/webpack/client.config",
        "build:client:prod": "cross-env NODE_ENV=production webpack --config src/config/webpack/client.config",
        "build:server:dev": "cross-env NODE_ENV=development webpack --config src/config/webpack/server.config",
        "build:server:prod": "cross-env NODE_ENV=production webpack --config src/config/webpack/server.config",
        "clear": "rimraf ./dist ./.nyc_output ./coverage",
        "coverage:report": "nyc report --reporter=text",
        "coverage:test": "cross-env TS_NODE_DISABLE_WARNINGS=true nyc mocha",
        "coverage:view": "opn coverage/index.html",
        "fixtypes": "cpr extra/index.d.ts node_modules/@types/express-session/index.d.ts -o",
        "graphiQLLocal": "cpr extra/renderGraphiQL.js node_modules/graphql-server-module-graphiql/dist/renderGraphiQL.js -o",
        "lint": "tslint -c tslint.json -p . --type-check",
        "lint:fix": "npm run lint -- --fix",
        "lint:report": "npm run lint -- -o report.txt --force; code report.txt",
        "postinstall": "npm run graphiQLLocal",
        "start": "cross-env DEBUGx=express-session,cookie node dist/server/server.js",
        "start:server": "cross-env DEBUGx=express-session,cookie node dist/server/server.js",
        "test": "cross-env TS_NODE_DISABLE_WARNINGS=true mocha",
        "test:w": "npm run test -- -w",
        "DISABLEDpre-test": "echo prestest",
        "DISABLEDpost-test": "echo posttest",
        "DISABLEDpreinstall": "echo preinstall",
        "DISABLEDinstall": "echo install",
        "DISABLEDprepare": "echo 'prepare'",
        "DISABLEDprepublish": "echo 'prepublish'",
        "DISABLEDpublish": "echo publish",
        "DISABLEDprepublishOnly": "echo prepublishOnly",
        "DISABLEDprerestart": "echo prerestart",
        "DISABLEDrestart": "echo restart",
        "DISABLEDpostrestart": "echo postrestart",
        "DISABLEDpreshrinkwrap": "echo preshrinkwrap",
        "DISABLEDshrinkwrap": "echo shrinkwrap",
        "DISABLEDpostshrinkwrap": "echo postshrinkwrap",
        "DISABLEDprestart": "echo prestart",
        "DISABLEDpoststart": "echo poststart",
        "DISABLEDprestop": "echo prestop",
        "DISABLEDstop": "echo stop",
        "DISABLEDpoststop": "echo poststop",
        "DISABLEDpreuninstall": "echo preuninstall",
        "DISABLEDuninstall": "echo uninstall",
        "DISABLEDpostuninstall": "echo postuninstall",
        "DISABLEDpreversion": "echo preversion",
        "DISABLEDversion": "echo version",
        "DISABLEDpostversion": "echo postversion"
    },
    "dependencies": {
        "apollo-client": "^1.9.2",
        "body-parser": "^1.18.1",
        "circular-json": "^0.3.3",
        "express": "^4.15.4",
        "express-session": "^1.15.5",
        "graphql": "^0.11.3",
        "graphql-server-core": "^1.1.0",
        "graphql-server-express": "^1.1.2",
        "graphql-tag": "^2.4.2",
        "graphql-tools": "^1.2.2",
        "install": "^0.10.1",
        "mailgun-js": "^0.13.1",
        "merge2": "^1.2.0",
        "moment": "^2.18.1",
        "npm": "^5.4.1",
        "pg": "^7.3.0",
        "pg-cursor": "^1.3.0",
        "pg-query-stream": "^1.1.1",
        "react": "^15.6.1",
        "react-apollo": "^1.4.15",
        "react-dom": "^15.6.1",
        "react-router-dom": "^4.2.2",
        "tracer": "^0.8.11",
        "uid-safe": "^2.1.5"
    },
    "devDependencies": {
        "@types/body-parser": "^1.16.5",
        "@types/chai": "^4.0.4",
        "@types/express": "^4.0.37",
        "@types/express-session": "^1.15.3",
        "@types/graphql": "^0.11.3",
        "@types/mocha": "^2.2.43",
        "@types/node": "^8.0.28",
        "@types/pg": "^7.1.0",
        "@types/pg-pool": "0.0.3",
        "@types/pg-query-stream": "^1.0.1",
        "@types/react": "^16.0.5",
        "@types/react-dom": "^15.5.4",
        "@types/react-router-dom": "^4.0.5",
        "@types/uid-safe": "^2.1.1",
        "awesome-typescript-loader": "^3.2.3",
        "babel-core": "^6.26.0",
        "babel-preset-env": "^1.5.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "chai": "^4.1.2",
        "clean-webpack-plugin": "^0.1.16",
        "cpr": "^2.0.2",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.7",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.11.2",
        "html-webpack-plugin": "^2.30.1",
        "html-webpack-template": "^5.6.0",
        "inline-manifest-webpack-plugin": "^3.0.1",
        "lerna": "^2.1.2",
        "mocha": "^3.5.3",
        "node-sass": "^4.5.3",
        "npm-check-updates": "^2.12.1",
        "nyc": "^11.2.1",
        "opn-cli": "^3.1.0",
        "postcss-cssnext": "^3.0.2",
        "postcss-loader": "^2.0.5",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.5",
        "source-map-support": "^0.4.18",
        "style-loader": "^0.18.1",
        "ts-node": "^3.3.0",
        "tsconfig-paths": "^2.3.0",
        "tslint": "^5.7.0",
        "tslint-loader": "^3.5.3",
        "typescript": "^2.5.2",
        "url-loader": "^0.5.9",
        "webpack": "^3.5.6",
        "webpack-node-externals": "^1.6.0"
    }
}
